{
  "name": "@qubit-ltd/common-api",
  "version": "3.3.0",
  "description": "A JavaScript library of encapsulation of common APIs",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-api.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-api/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-api",
  "keywords": [
    "JavaScript",
    "API"
  ],
  "main": "dist/common-api.min.cjs",
  "module": "dist/common-api.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-api.min.cjs",
      "import": "./dist/common-api.min.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist/api",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run doc && npm run build:dev && npm run build",
    "test": "jest",
    "lint": "eslint",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/'",
    "deploy:all": "npm run deploy"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.26.0",
    "@qubit-ltd/common-app": "^1.11.17",
    "@qubit-ltd/common-decorator": "^3.10.2",
    "@qubit-ltd/common-filter": "^1.10.2",
    "@qubit-ltd/common-model": "^4.18.2",
    "@qubit-ltd/common-normalizer": "^1.8.2",
    "@qubit-ltd/common-ui": "^1.14.2",
    "@qubit-ltd/common-util": "^5.4.1",
    "@qubit-ltd/config": "^1.2.1",
    "@qubit-ltd/logging": "^1.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.3",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/clone": "^1.13.1",
    "@qubit-ltd/common-app": "^1.11.20",
    "@qubit-ltd/common-decorator": "^3.10.3",
    "@qubit-ltd/common-filter": "^1.10.2",
    "@qubit-ltd/common-model": "^4.20.0",
    "@qubit-ltd/common-normalizer": "^1.8.3",
    "@qubit-ltd/common-ui": "^1.14.2",
    "@qubit-ltd/common-util": "^5.4.3",
    "@qubit-ltd/common-validation-rule": "^1.6.1",
    "@qubit-ltd/common-validator": "^4.10.0",
    "@qubit-ltd/config": "^1.2.2",
    "@qubit-ltd/eslint-config": "^1.3.5",
    "@qubit-ltd/jsdoc-theme": "^1.5.0",
    "@qubit-ltd/json": "^1.2.2",
    "@qubit-ltd/logging": "^1.4.10",
    "@qubit-ltd/naming-style": "^1.3.7",
    "@qubit-ltd/pinia-decorator": "^3.4.2",
    "@qubit-ltd/rollup-builder": "^1.8.12",
    "@qubit-ltd/storage": "^1.4.2",
    "@qubit-ltd/type-detect": "^2.2.1",
    "@qubit-ltd/typeinfo": "^3.3.1",
    "@qubit-ltd/vue3-class-component": "^2.4.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.11.0",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "chai": "^6.0.1",
    "check-es-version": "^1.5.1",
    "core-js": "^3.45.1",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^6.0.0",
    "jsdoc": "^4.0.4",
    "pinia": "^3.0.3",
    "rollup": "^4.48.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.9.2",
    "vue": "^3.5.20"
  },
  "packageManager": "yarn@4.5.0"
}
