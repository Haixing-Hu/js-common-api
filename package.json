{
  "name": "@qubit-ltd/common-api",
  "version": "2.8.0",
  "description": "A JavaScript library of encapsulation of common APIs",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-api.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-api/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-api",
  "keywords": [
    "JavaScript",
    "API"
  ],
  "main": "dist/common-api.min.cjs",
  "module": "dist/common-api.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-api.min.cjs",
      "import": "./dist/common-api.min.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run doc && npm run build:dev && npm run build",
    "test": "jest",
    "lint": "eslint",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.26.0",
    "@qubit-ltd/common-app": "^1.11.6",
    "@qubit-ltd/common-decorator": "^3.8.10",
    "@qubit-ltd/common-filter": "^1.8.5",
    "@qubit-ltd/common-model": "^4.12.0",
    "@qubit-ltd/common-ui": "^1.12.0",
    "@qubit-ltd/common-util": "^5.3.4",
    "@qubit-ltd/config": "^1.1.2",
    "@qubit-ltd/logging": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/clone": "^1.12.3",
    "@qubit-ltd/common-app": "^1.11.6",
    "@qubit-ltd/common-decorator": "^3.8.10",
    "@qubit-ltd/common-filter": "^1.8.5",
    "@qubit-ltd/common-model": "^4.12.0",
    "@qubit-ltd/common-normalizer": "^1.7.6",
    "@qubit-ltd/common-ui": "^1.12.0",
    "@qubit-ltd/common-util": "^5.3.4",
    "@qubit-ltd/common-validation-rule": "^1.4.4",
    "@qubit-ltd/common-validator": "^4.7.6",
    "@qubit-ltd/config": "^1.1.2",
    "@qubit-ltd/eslint-config": "^1.3.4",
    "@qubit-ltd/jsdoc-minami": "^1.5.2",
    "@qubit-ltd/json": "^1.1.4",
    "@qubit-ltd/logging": "^1.4.5",
    "@qubit-ltd/naming-style": "^1.3.4",
    "@qubit-ltd/pinia-decorator": "^3.3.2",
    "@qubit-ltd/rollup-builder": "^1.8.9",
    "@qubit-ltd/storage": "^1.3.3",
    "@qubit-ltd/type-detect": "^1.2.3",
    "@qubit-ltd/typeinfo": "^3.1.4",
    "@qubit-ltd/vue3-class-component": "^2.3.5",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.7.9",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.40.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.2",
    "jsdoc": "^4.0.4",
    "pinia": "^2.3.1",
    "rollup": "^4.31.0",
